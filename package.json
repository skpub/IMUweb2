    "minecraft-protocol": "^1.62.0",
    "next": "16.1.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-aria-components": "^1.14.0",
    "react-dom": "19.2.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
